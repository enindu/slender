{
    "name": "enindu/slender",
    "description": "Developer-friendly, low-level, rapid web development environment, based on Slim framework",
    "keywords": [
        "php",
        "slender",
        "slim",
        "php-di",
        "eloquent",
        "twig",
        "swift-mailer",
        "parsedown",
        "framework",
        "cms"
    ],
    "version": "0.5.1",
    "license": "MIT",
    "homepage": "https://github.com/enindu/slender",
    "readme": "https://github.com/enindu/slender#readme",
    "authors": [
        {
            "name": "Enindu Alahapperuma",
            "email": "enindu@gmail.com",
            "homepage": "https://enindu.com",
            "role": "Developer"
        }
    ],
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "slim/slim": "^4.8",
        "slim/psr7": "^1.4",
        "php-di/php-di": "^6.3",
        "illuminate/database": "^8.53",
        "illuminate/events": "^8.53",
        "twig/twig": "^3.3",
        "twig/intl-extra": "^3.3",
        "swiftmailer/swiftmailer": "^6.2",
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.8",
        "intervention/image": "^2.6",
        "rakit/validation": "^1.4",
        "symfony/filesystem": "^5.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "System\\": "system"
        }
    }
}
