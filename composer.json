{
    "name": "enindu/slender",
    "description": "Developer-friendly, low-level, rapid web development environment, based on Slim framework",
    "keywords": [
        "php",
        "slender",
        "slim",
        "php-di",
        "twig"
    ],
    "version": "0.3.0-dev",
    "license": "MIT",
    "homepage": "https://github.com/enindu/slender",
    "readme": "https://github.com/enindu/slender#readme",
    "authors": [
        {
            "name": "Enindu Alahapperuma",
            "email": "me@enindu.com",
            "homepage": "https://enindu.com",
            "role": "Developer"
        }
    ],
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "slim/slim": "^4.7",
        "slim/psr7": "^1.3",
        "php-di/php-di": "^6.3",
        "twig/twig": "^3.2",
        "twig/cssinliner-extra": "^3.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.2",
        "devster/ubench": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "System\\": "system"
        }
    }
}
